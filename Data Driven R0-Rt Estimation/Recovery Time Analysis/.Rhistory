library("fitdistrplus")
setwd(".")
reporting = read.csv("reporting_delay.csv")
fitvalues = as.numeric(unlist(c(reporting['lag'])))
maxx = max(fitvalues)
minn = min(fitvalues)
#fitvalues = (fitvalues-min(fitvalues))/(max(fitvalues)-min(fitvalues))
png(file="description.png",width = 4, height = 4, res = 300, units = 'in',pointsize = 6)
descdist(fitvalues, boot = 1000)
dev.off()
fg <- fitdist(fitvalues, "gamma")
fe <- fitdist(fitvalues, "exp")
plot.legend <- c("gamma","exponential")
png(file="fit.png",width = 4, height = 4, res = 300, units = 'in',pointsize = 6)
par(mfrow = c(2, 2))
denscomp(list(fg,fe), legendtext = plot.legend)
qqcomp(list(fg,fe), legendtext = plot.legend)
cdfcomp(list(fg,fe), legendtext = plot.legend)
ppcomp(list(fg,fe), legendtext = plot.legend)
dev.off()
gofstat(list(fg,fe), fitnames = c("gamma","exponential"))
summary(bootdist(fg, niter = 1001))
summary(bootdist(fe, niter = 1001))
fg
